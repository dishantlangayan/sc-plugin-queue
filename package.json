{
  "name": "@dishantlangayan/sc-plugin-queue",
  "description": "Commands to interact with queues on a Solace Cloud broker",
  "version": "0.1.1",
  "author": "Dishant Langayan",
  "bin": {
    "sc": "./bin/run.js"
  },
  "bugs": "https://github.com/dishantlangayan/sc-plugin-queue/issues",
  "dependencies": {
    "@dishantlangayan/sc-cli-core": "^0.1.2",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^21.0.0",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^21.0.1",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/dishantlangayan/sc-plugin-queue",
  "keywords": [
    "oclif",
    "solace",
    "cli",
    "sc",
    "sc-plugin"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "sc",
    "dirname": "sc",
    "commands": "./dist/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "dishantlangayan/sc-plugin-queue",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
